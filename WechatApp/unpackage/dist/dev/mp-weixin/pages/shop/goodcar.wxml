<view><block wx:if="{{goods.length===0}}"><view class="empty"><image style="width:400rpx;" src="https://bpic.588ku.com/element_origin_min_pic/19/04/10/761ee07a08419d9db3c24bd05d88e360.jpg" mode="widthFix"></image><view class="empty-text">空空如也的购物</view><view data-event-opts="{{[['tap',[['goshopping',['$event']]]]]}}" class="empty-button" bindtap="__e">去选购</view></view></block><block wx:else><view class="goods-box"><block wx:for="{{goods}}" wx:for-item="item" wx:for-index="index" wx:key="id"><view class="goods-detail"><view class="detail-left"><view class="goods-left"><checkbox-group><label><checkbox class="selected" color="#ff5500" checked="{{item.flag}}" data-event-opts="{{[['tap',[['djxz',['$0'],[[['goods','id',item.id]]]]]]]}}" bindtap="__e"></checkbox><text></text></label></checkbox-group><image style="width:150rpx;height:140rpx;margin-left:20rpx;" src="{{item.image}}" alt="显示失败" class="_img"></image></view><view class="GoodsName"><text style="font-GoodsName:25rpx;">{{item.desc}}</text><text class="goods-price">{{"￥"+item.title+"/件"}}</text></view></view><view class="detail-right"><text data-event-opts="{{[['tap',[['reduce',['$0'],[[['goods','id',item.id]]]]]]]}}" class="subtract" bindtap="__e">-</text><text class="num">{{item.num}}</text><text data-event-opts="{{[['tap',[['add',['$0'],[[['goods','id',item.id]]]]]]]}}" class="add" bindtap="__e">+</text><view data-event-opts="{{[['tap',[['goodsDel',['$0'],[[['goods','id',item.id]]]]]]]}}" class="goods-del" bindtap="__e"><uni-icons vue-id="{{'5747cbe2-1-'+index}}" type="trash-filled" size="20" bind:__l="__l"></uni-icons></view></view></view></block><view class="end"><view class="end-left"><checkbox-group data-event-opts="{{[['change',[['selectgoods']]]]}}" bindchange="__e"><label><checkbox checked="{{allchecked}}"></checkbox>全选</label></checkbox-group><view>总计：<text style="color:#f00;font-weight:bold;">{{"￥ "+totalPrice}}</text></view></view><view data-event-opts="{{[['tap',[['buy',['$event']]]]]}}" class="end-right" bindtap="__e">结算</view></view></view></block></view>